{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Results",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "exposure": { "type": "string" },
      "outcome": { "type": "string" },
      "beta": { "type": ["number", "null"] },
      "units": { "type": ["string", "null"] },
      "odds ratio": { "type": ["number", "null"] },
      "hazard ratio": { "type": ["number", "null"] },
      "95% CI": {
        "type": ["array", "null"],
        "items": { "type": ["number", "null"] },
        "minItems": 2,
        "maxItems": 2
      },
      "SE": { "type": ["number", "null"] },
      "P-value": { "type": ["number", "null"] },
      "direction": { "type": ["string", "null"] }
    },
    "required": [
      "exposure",
      "outcome",
      "beta",
      "units",
      "odds ratio",
      "hazard ratio",
      "95% CI",
      "SE",
      "P-value",
      "direction"
    ],
    "additionalProperties": false
  }
}
